<?xml version="1.0" encoding="UTF-8" ?>
<unitList
        xmlns="http://www.xml-cml.org/schema"
        xmlns:dc="http://purl.org/dc/elements/1.1/"
        xmlns:dummyDictionary="http://www.xml-cml.org/dictionary/"
        xmlns:convention="http://www.xml-cml.org/convention/"
        xmlns:xhtml="http://www.w3.org/1999/xhtml"
        convention="convention:unit-dictionary"
        namespace="http://www.xml-cml.org/dictionary/dummy/"
        title="example units dictionary"
        >
    <dc:title>test file for http://www.xml-cml.org/convention/unit-dictionary convention</dc:title>
    <dc:description>a unit must have a symbol attribute - the value of which is the full symbol used to represent this unit</dc:description>
    <dc:author>J A Townsend</dc:author>
    <dc:rights>Copyright J A Townsend jat45@cantab.net 2011.</dc:rights>
    <dc:date>2011-03-03</dc:date>
    <description>
        <xhtml:p>
            A dummy units dictionary.
        </xhtml:p>
    </description>
    <unit
            title="unit 1"
            id="u1"
            parentSI="dummyDictionary:dummy"
            multiplierToSI="1"
            constantToSI="0"
            unitType="dummyDictionary:dummy">
        <dc:source>a dummy source</dc:source>
        <definition>
            <xhtml:p>
                The description of the first entry in this dummy unit dictionary.
            </xhtml:p>
        </definition>
        <description>
            <xhtml:p>
                The description of the first unit in this dummy unit dictionary.
            </xhtml:p>
        </description>
    </unit>
    <unit
            title="unit 2"
            id="u2"
            symbol="unit2"
            parentSI="dummyDictionary:dummy"
            constantToSI="1"
            unitType="dummyDictionary:dummy">
        <dc:source>a dummy source</dc:source>
        <definition>
            <xhtml:p>
                The description of the second entry in this dummy unit dictionary.
            </xhtml:p>
        </definition>
        <description>
            <xhtml:p>
                The description of the second unit in this dummy unit dictionary.
            </xhtml:p>
        </description>
    </unit>
    <unit
            title="unit 3"
            id="u3"
            symbol="unit3"
            parentSI="dummyDictionary:dummy"
            multiplierToSI="2"
            unitType="dummyDictionary:dummy">
        <dc:source>a dummy source</dc:source>
        <definition>
            <xhtml:p>
                The description of the third entry in this dummy unit dictionary.
            </xhtml:p>
        </definition>
        <description>
            <xhtml:p>
                The description of the third unit in this dummy unit dictionary.
            </xhtml:p>
        </description>
    </unit>
</unitList>

