<?xml version="1.0" encoding="utf-8" ?>
<cml:cml xmlns:cml="http://www.xml-cml.org/schema"
         xmlns:html="http://www.w3.org/1999/xhtml"
         xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:conventions="http://www.xml-cml.org/convention/"
         xmlns:cmlDictionary="http://www.xml-cml.org/dictionary/"
         convention="conventions:dictionary">
    <dc:title>test file for http://www.xml-cml.org/convention/dictionary convention</dc:title>
    <dc:description>if the dictionary convention is declared on the "cml" element then there must be at least one
        "dictionary" element as a direct child
    </dc:description>
    <dc:author>J A Townsend</dc:author>
    <dc:rights>Copyright J A Townsend jat45@cantab.net 2009.</dc:rights>
    <dc:date>2010-12-23</dc:date>
    <foreign-element>
        <cml:dictionary convention="conventions:dictionary" title="a dummy dictionary"
                        namespace="http://www.xml-cml.org/dictionary/cml">
            <cml:entry id="e1" term="first entry" title="first entry" dataType="xsd:string">
                <definition>
                    <html:p>
                        A definition
                    </html:p>
                </definition>
                <cml:description>
                    <html:p>
                        the first entry in the dictionary
                    </html:p>
                </cml:description>
            </cml:entry>
        </cml:dictionary>
    </foreign-element>
</cml:cml>