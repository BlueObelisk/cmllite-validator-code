<?xml version="1.0" encoding="UTF-8"?>
<cml xmlns="http://www.xml-cml.org/schema"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:conventions="http://www.xml-cml.org/convention/" convention="conventions:cmllite">
    <dc:title>CMLLite test file</dc:title>
    <dc:description>valid CMLLite should not fail becuase although two atoms have same id they are in different
        (sibling) molecules
    </dc:description>
    <dc:author>J A Townsend</dc:author>
    <dc:rights>Copyright J A Townsend jat45@cantab.net 2009.</dc:rights>
    <dc:date>2009-01-21</dc:date>
    <molecule id="m1">
        <atomArray>
            <atom id="a1" elementType="H"/>
        </atomArray>
    </molecule>
    <molecule id="m2" count="1">
        <atomArray>
            <atom id="a1" elementType="He"/>
        </atomArray>
    </molecule>
</cml>
