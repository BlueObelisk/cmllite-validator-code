<?xml version="1.0" encoding="UTF-8"?>
<cml:cml xmlns:cml="http://www.xml-cml.org/schema"
         xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:conventions="http://www.xml-cml.org/convention/" convention="conventions:cmllite">
    <dc:title>CMLLite test file</dc:title>
    <dc:description>both the refs in a bond's atomRefs2 must point to atoms within the eldest containing molecule
    </dc:description>
    <dc:author>J A Townsend</dc:author>
    <dc:rights>Copyright J A Townsend jat45@cantab.net 2009.</dc:rights>
    <dc:date>2009-01-21</dc:date>
    <cml:molecule id="m1">
        <cml:atomArray>
            <cml:atom id="a1" elementType="H"/>
        </cml:atomArray>
        <cml:bondArray>
            <cml:bond id="b1" atomRefs2="a1 a2"/>
        </cml:bondArray>
    </cml:molecule>
    <cml:molecule id="m2">
        <cml:atomArray>
            <cml:atom id="a2" elementType="Cl"/>
        </cml:atomArray>
    </cml:molecule>
</cml:cml>